<launch>
    <env name="ROSCONSOLE_CONFIG_FILE" value="$(find point_lio)/config/rosconsole.conf" />
    <node pkg="point_lio" type="point_lio_node" name="point_lio" output="screen">
        <param name="lidar_meas_cov_inv" value="1000"/>
        <param name="imu_meas_acc_cov_inv" value="10"/>
        <param name="imu_meas_gyro_cov_inv" value="10"/>
        <param name="filter_num" value= "6"/>
        <param name="map_resolution" value= "0.25"/>
    </node>
    <node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find point_lio)/rviz/default.rviz" />
</launch>